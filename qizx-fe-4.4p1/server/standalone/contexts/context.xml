<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Configure PUBLIC "-//Jetty//Configure//EN" 
                           "http://www.eclipse.org/jetty/configure.dtd">
<!-- DO NOT EDIT THIS FILE BY HAND! -->
<Configure class="org.eclipse.jetty.webapp.WebAppContext">

  <Set name="contextPath">/qizx</Set>
  <Set name="war"><SystemProperty name="jetty.home" default="."/>/../qizx</Set>

  <Get name="securityHandler">
    <Set name="loginService">
      <New class="org.eclipse.jetty.security.HashLoginService">
        <Set name="name">Qizx XML Server</Set>
	<Set name="config"><SystemProperty name="jetty.home" default="."/>/etc/users.properties</Set>
      </New>
    </Set>
  </Get>

</Configure>
